local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Prison Life | Mod Menu", IntroName = "Mod Menu", HidePremium = false, SaveConfig = true, ConfigFolder = "PLMM"})

local PTab = Window:MakeTab({
	Name = "Player Menu",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = PTab:AddSection({
	Name = "Walk Speed"
)}

PTab:AddSlider({
	Name = "Walk Speed",
	Min = 0,
	Max = 300,
	Default = 16,
	Color = Color3.fromRGB(255,0,10),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (Value)
	end    
})

local Section = PTab:AddSection({
  Name = "Jump Power"
)}

PTab:AddSlider({
  Name = "Jump Power",
  Min = 5,
  Max = 200,
  Default = 50,
  Color = Color3.fromRGB(255,0,10),
  Increment = 1,
  ValueName = "Power",
  CallBack = function(Value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = (Value)

local Section = PTab:AddSection({
	Name = "Infinite Jump",
})

PTab:AddToggle({
	Name = "Infinite Jump",
	Default = false,
	Callback = function(Value)
		Value = pcall(loadstring(game:HttpGet("https://pastebin.com/raw/2wgbZ6Xd")))

local Section = Tab:AddSection({
	Name = "Notification Toggle"
})

PTab:AddToggle({
	Name = "Notifications",
	Default = false,
	Callback = function(Value)
		Value = loadstring(game:HttpGet("https://github.com/HydroHehe/Notification/blob/main/Code)))
})

local Tab = Window:MakeTab({
	Name = "Prison Life",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Kill All"
})

Tab:AddButton({
	Name = "Kill",
	Callback = function()
      		function = loadstring(game:HttpGet("https://pastebin.com/raw/vNLbSj28")))
  	end    
})

local Section = Tab:AddSection({
	Name = "Gun Mod"
})

Tab:AddButton({
	Name = "Button!",
	Callback = function()
      		function = loadstring(game:HttpGet("https://pastebin.com/raw/PxA2ezs0")))
  	end    
})

