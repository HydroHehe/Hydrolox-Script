local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Prison Life | Mod Menu", IntroName = "Mod Menu", HidePremium = false, SaveConfig = true, ConfigFolder = "PLMM"})

local PTab = Window:MakeTab({
	Name = "Player Menu",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = PTab:AddSection({
	Name = "Walk Speed"
)}

PTab:AddSlider({
	Name = "Walk Speed",
	Min = 0,
	Max = 300,
	Default = 16,
	Color = Color3.fromRGB(255,0,10),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (Value)
	end    
})

local Section = PTab:AddSection({
  Name = "Jump Power"
)}

PTab:AddSlider({
  Name = "Jump Power",
  Min = 5,
  Max = 200,
  Default = 50,
  Color = Color3.fromRGB(255,0,10),
  Increment = 1,
  ValueName = "Power",
  CallBack = function(Value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = (Value)


local Section = Tab:AddSection({
	Name = "Gun Mod"
})

Tab:AddButton({
	Name = "Mod",
	Callback = function()
      	  loadstring(game:HttpGet((https://pastebin.com/raw/PxA2ezs0)))
  	end    
})

local Tab = Window:MakeTab({
	Name = "Pro || Mod Menu",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = true
})

local Tab = Window:MakeTab({
	Name = "GUI",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Teleport GUI"
})

Tab:AddButton({
	Name = "Open GUI",
	Callback = function()
      		loadstring(game:HttpGet((https://pastebin.com/raw/816JqDn8)))
  	end    
})

local Section = Tab:AddSection({
	Name = "Tiger Admin GUI"
})

Tab:AddButton({
	Name = "Open GUI",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/H17S32/Tiger_Admin/main/Script'))() 
  	end    
})



OrionLib:Init()
